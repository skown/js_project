{
    "compilerOptions": {
      "strict": true,
      "target": "esnext",
      "moduleResolution": "node",
      "declaration": true,
      "emitDeclarationOnly": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "baseUrl": "./"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }